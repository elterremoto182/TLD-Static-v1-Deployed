{
  "redirects": [
    {
      "source": "/sitemap.xml",
      "destination": "/sitemaps/index.xml",
      "permanent": true
    },
    {
      "source": "/leak-detection-services/",
      "destination": "/services/leak-detection/",
      "permanent": true
    },
    {
      "source": "/how-to-check-a-shower-pan-for-leaks/",
      "destination": "/how-to-check-shower-pan-leaks/",
      "permanent": true
    },
    {
      "source": "/services/leak-detection-services/",
      "destination": "/services/leak-detection/",
      "permanent": true
    },
    {
      "source": "/services/mold-detectionl-and-remediation/",
      "destination": "/services/mold-testing/",
      "permanent": true
    },
    {
      "source": "/reviews/",
      "destination": "/testimonials/",
      "permanent": true
    }
  ],
  "headers": [
    {
      "source": "/_next/static/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/nextImageExportOptimizer/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/images/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/wp-content/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    },
    {
      "source": "/(.*\\.(woff2?|ttf|otf|eot))",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "public, max-age=31536000, immutable"
        }
      ]
    }
  ]
}

